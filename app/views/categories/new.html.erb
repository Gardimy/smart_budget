<div class="cont">
  <%= form_with model: @categories, url: categories_path, method: :post, local: true do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name, placeholder: "Transport" %>
    </div>
    <% icon_options = [
           ['Transport', 'data:image/png;base64,'],
            ['Fast food', 'https://cdn-icons-png.flaticon.com/512/2819/2819194.png'],
            ['Clothing', 'data:image/png;base64,'],
            ['Shopping', 'data:image/png;base64,'],
            ['Health', 'data:image/png;base64,'],
            ['Utility', 'data:image/jpeg;base64,'],
            ['Education', 'data:image/png;base64,'],
            ['Entertainment', 'data:image/png;base64,'],
  
    ]  %>
    <div>
      <%= f.label :icon %>
      <%= f.select :icon, options_for_select(icon_options), prompt: "Select an icon", id: "icon-select" %>
    </div>
    <div>
    <% if @categories.icon.present? %>
      <% selected_option = icon_options.find { |option| option[1] == @categories.icon } %>
      <% if selected_option %>
        <%= image_tag selected_option[1], alt: "Selected icon" %>
      <% end %>
    <% end %>
  </div>
    <div>
      <%= f.submit "Add Category" %>
    </div>
  <% end %>
  <%= link_to 'Back', categories_path %>
</div>
